<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
	<LteCellConfigPath>/mt_wlyh/Data/config/tb_cfg_city_cell.txt</LteCellConfigPath>
	<MreDataPath>/mt_wlyh/Data/mre/${yyMMdd}</MreDataPath>
	<MroXdrMergePath>/mt_wlyh/Data/mroxdrmerge</MroXdrMergePath>
	<MroDataPath>/mt_wlyh/Data/mro</MroDataPath>
	<MTMroDataPath>/mt_wlyh/Data/mromt/${yyMMdd}</MTMroDataPath>
	<OrigLocationPath>/flume/location</OrigLocationPath>
	<PigBinPath>/opt/pig-0.15.0/bin</PigBinPath>
	<OrigXdrPath>/flume/xdr</OrigXdrPath>
	<XdrDataPath>/result/test</XdrDataPath>
	<GsmCellConfigPath>/mt_wlyh/Data/config/tb_cfg_city_cell_gsm.txt</GsmCellConfigPath>
	<TDCellConfigPath>/mt_wlyh/Data/config/tb_cfg_city_cell_td.txt</TDCellConfigPath>
	<FigureConfigPath>/mt_wlyh/Data/config/cellfigure</FigureConfigPath>
	<SimuLocConfigPath>/mt_wlyh/Data/config</SimuLocConfigPath>
	<imeiCapbilityPath>/mt_wlyh/Data/config/imei_learn.txt</imeiCapbilityPath>
	<!-- reduce 的数量 -->
	<reduceNmu>1000</reduceNmu>
	<!--前后扩展场强的范围 -->
	<range>6</range>
	<!--栅格中至少至少匹配到的小区数，小于这个数丢弃该栅格 -->
	<CellNum>1</CellNum>
	<!-- -经度加减随机数范围 -->
	<ilongitudRrandomWidth>0.0002</ilongitudRrandomWidth>
	<!-- -纬度加减随机数范围 -->
	<ilatitudeRrandomWidth>0.00018</ilatitudeRrandomWidth>
	<pianyiNum>1</pianyiNum>
	<!-- cqt、dt 划分比例分界线 -->
	<percent>0.51</percent>
	<!-- figureFixFlag 是否用指纹库定位 -->
	<figureFixFlag>1</figureFixFlag>
	<!-- 单个map分配的核心数 -->
	<mapVcore>1</mapVcore>
	<!-- 单个reduce分配的核心数 -->
	<reduceVcore>1</reduceVcore>
	<!-- lzo库路径 -->
	<lzoPath>/usr/local/lzo/lib</lzoPath>
	<!-- map内存大小 兆为单位 -->
	<mapMemory>4094</mapMemory>
	<!-- reduce内存大小 兆为单位 -->
	<reduceMemory>8192</reduceMemory>
	
	<dealSizeMap_M>1024</dealSizeMap_M>
	<dealSizeReduce_M>1536</dealSizeReduce_M>
	<slowReduce>1</slowReduce>
	<failPrecentMap>10</failPrecentMap>
	<failPrecentReduce>10</failPrecentReduce>
			<!--事件统计命令-->
	<httpPath>/seq/news1u_http_cut/20%1$s</httpPath> 
	<mwPath>/AA/seq/Mw/20%1$s</mwPath>
	<svPath>/AA/seq/Sv/20%1$s</svPath>
	<rxPath>/AA/seq/Rx/20%1$s</rxPath>
	<mosPath>/A%1$s/xdr_abnormal/20%2$s/mosquality</mosPath>
	<dhwjtPath>/A%1$s/xdr_abnormal/20%2$s/misstell</dhwjtPath>
	<rtpPath></rtpPath>
	<mgPath></mgPath>
	<mmePath>/seq/news1mme_cut/20%1$s</mmePath>
	<imsMoPath>/A/seq/cdr_ims_mo/20%1$s</imsMoPath>
	<imsMtPath>/A/seq/cdr_ims_mt/20%1$s</imsMtPath>
	<quaLityPath>/A/seq/cdr_volte/20%1$s</quaLityPath>
	
	<!-- 是否输出源数据，加上定位信息  是填写yes,默认为false -->
	<outXdrData>no</outXdrData>
	<!-- 是否输出 event统计  是填写yes,默认为false -->
	<toEventData>yes</toEventData>
	
	<outSuccessData>no</outSuccessData>
	<outFailData>no</outFailData>
	<!-- m是否输出mme重定向，默认为false -->
	<mme23GRedirect>true</mme23GRedirect>
	<!--高铁相关配置-->
	<RailCellConf>/mt_wlyh/Data/config/rail/cellconf</RailCellConf>
	<RailConf>/mt_wlyh/Data/config/rail/railconf</RailConf>
	<RailRailStation>/mt_wlyh/Data/config/rail/stationconf/railstation.txt</RailRailStation>
	<RailRRU>/mt_wlyh/Data/config/rail/rruconf/rru.txt</RailRRU>
	<!-- 小区楼宇关系表所文件夹 -->
	<cellBuildPath>/mt_wlyh/Data/config/cellBuildConfig</cellBuildPath>
	<cellBuildWifiPath>/mt_wlyh/Data/config/wifiBuildingConfig</cellBuildWifiPath>
	
	<MergeUserResidentDayNum>-30</MergeUserResidentDayNum>
	<!-- 楼宇楼层室分用户数配置 -->
	<BuildIndoorCellUserConfigPath>/mt_wlyh/Data/mroxdrmerge/resident_user/tb_cfg_building_indoor_cell</BuildIndoorCellUserConfigPath>
	<!-- 家宽配置 -->
	<HomeBroadbandConfigPath>/mt_wlyh/Data/config/homebroadband.txt</HomeBroadbandConfigPath>

	<!-- 高铁区间小区配置 -->
	<hsrSectionCellPath>/mt_wlyh/Data/config/hsr/tb_hsr_cfg_section_cell.txt</hsrSectionCellPath>
	<!-- 高铁站小区配置 -->
	<hsrStationCellPath>/mt_wlyh/Data/config/hsr/tb_hsr_cfg_station_cell.txt</hsrStationCellPath>
	<!-- 高铁站配置 -->
	<hsrStationPath>/mt_wlyh/Data/config/hsr/tb_hsr_cfg_station.txt</hsrStationPath>
	<!-- 高铁区间配置 -->
	<hsrSectionPath>/mt_wlyh/Data/config/hsr/tb_hsr_cfg_section.txt</hsrSectionPath>
	<!-- 高铁路段配置 -->
	<hsrSegmentPath>/mt_wlyh/Data/config/hsr/tb_hsr_cfg_segment.txt</hsrSegmentPath>
	<!-- 高铁RRU配置 -->
	<hsrSectionRruPath>/mt_wlyh/Data/config/hsr/tb_hsr_cfg_rru.txt</hsrSectionRruPath>
</configuration>

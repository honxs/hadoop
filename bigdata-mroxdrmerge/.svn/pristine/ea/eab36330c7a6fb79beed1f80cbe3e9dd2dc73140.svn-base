package cn.mastercom.bigdata.loc.area;

public class HSRAreaInfo {
	public long imsi;
	public String imei;
	public int areaType;
	public int areaID;//1候车厅2站台3驶入4停经5驶离6出站
	public int stime;//场景开始时间
	public int etime;//场景结束时间
	public int stationID;
	public HSRAreaInfo() {
		super();
	}
	public HSRAreaInfo(int areaType, int areaID, int stime,
			int etime, int stationID) {
		super();
		this.areaType = areaType;
		this.areaID = areaID;
		this.stime = stime;
		this.etime = etime;
		this.stationID = stationID;
	}
	
}

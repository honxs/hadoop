<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
	<LteCellConfigPath>/wangyou/mastercom/config/tb_cfg_city_cell.txt</LteCellConfigPath>
	<MreDataPath>/wangyou/mastercom/mre/${yyMMdd}</MreDataPath>
	<MroXdrMergePath>/wangyou/mastercom/mroxdrmerge</MroXdrMergePath>

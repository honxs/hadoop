	<MTMroDataPath>/sichuan/ods/ns/MR/MRO/ERICSSON/${yyyyMMdd},/sichuan/ods/ns/MR/MRO/HUAWEI/${yyyyMMdd},/sichuan/ods/ns/MR/MRO/NSN/${yyyyMMdd},/sichuan/ods/ns/MR/MRO/ZTE/${yyyyMMdd}</MTMroDataPath>
	<OrigLocationPath>/wangyou/mastercom/location</OrigLocationPath>
	<PigBinPath>/opt/pig-0.15.0/bin</PigBinPath>
	<OrigXdrPath>/wangyou/mastercom/orgxdr</OrigXdrPath>
	<XdrDataPath>/wangyou/mastercom/xdr</XdrDataPath>
	<GsmCellConfigPath>/wangyou/mastercoma/config/tb_cfg_city_cell_gsm.txt</GsmCellConfigPath>
	<TDCellConfigPath>/wangyou/mastercom/config/tb_cfg_city_cell_td.txt</TDCellConfigPath>
	<FigureConfigPath>/wangyou/mastercom/config</FigureConfigPath>
	<SimuLocConfigPath>/wangyou/mastercom/config/simu</SimuLocConfigPath>
	<!--高铁相关配置-->
	<RailCellConf>/wangyou/mastercom/rail/cellconf</RailCellConf>
	<RailConf>/wangyou/mastercom/rail/railconf</RailConf>
	<RailRailStation>/wangyou/mastercom/rail/stationconf/railstation.txt</RailRailStation>
	<RailRRU>/wangyou/mastercom/rail/rruconf/rru.txt</RailRRU>
	<path_ImsiCellLocPath></path_ImsiCellLocPath>
	<LocWFDataPath>/wangyou/mastercom/mroxdrmerge/wlanLoc/${yyMMdd}</LocWFDataPath>
	<!-- reduce 的数量 -->
	<reduceNmu>1000</reduceNmu>
	<!--前后扩展场强的范围 -->
	<range>6</range>
	<!--栅格中至少至少匹配到的小区数，小于这个数丢弃该栅格 -->
	<CellNum>1</CellNum>
	<!-- -经度加减随机数范围 -->
	<ilongitudRrandomWidth>0.0002</ilongitudRrandomWidth>
	<!-- -纬度加减随机数范围 -->
	<ilatitudeRrandomWidth>0.00018</ilatitudeRrandomWidth>
	<pianyiNum>1</pianyiNum>
	<!-- cqt、dt 划分比例分界线 -->
	<percent>0.51</percent>
	<!-- figureFixFlag 是否用指纹库定位 -->
	<figureFixFlag>1</figureFixFlag>
	<!-- 单个map分配的核心数 -->
	<mapVcore>2</mapVcore>
	<!-- 单个reduce分配的核心数 -->
	<reduceVcore>2</reduceVcore>
	<!-- lzo库路径 -->
	<lzoPath>/usr/local/lzo/lib</lzoPath>
	<!-- map内存大小 兆为单位 -->
	<mapMemory>6144</mapMemory>
	<!-- reduce内存大小 兆为单位 -->
	<reduceMemory>8192</reduceMemory>
	<huaweiMdtLogDataPath>/sichuan/ods/wy/MDT/HW/LOG/${yyyyMMdd}</huaweiMdtLogDataPath>
	<huaweiMdtImmDataPath>/sichuan/ods/wy/MDT/HW/IMM-MM/${yyyyMMdd}</huaweiMdtImmDataPath>
	<zteMdtImmDataPath>/sichuan/ods/wy/MDT/ZX/IMM-M1/${yyyyMMdd}</zteMdtImmDataPath>
	<zteMdtLogDataPath>/sichuan/ods/wy/MDT/ZX/LOG/${yyyyMMdd}</zteMdtLogDataPath>
	<cellBuildPath>/wangyou/mastercom/config/cellBuildConfig</cellBuildPath>
	<cellBuildWifiPath>/wangyou/mastercom/config/cellWifiConfig</cellBuildWifiPath>
	<imeiCapbilityPath>/wangyou/mastercom/config/imei_learn.txt</imeiCapbilityPath>
	<dealSizeMap_M>512</dealSizeMap_M>
	<dealSizeReduce_M>2048</dealSizeReduce_M>
	<slowReduce>0.99</slowReduce>
	<failPrecentMap>5</failPrecentMap>
	<failPrecentReduce>5</failPrecentReduce>
	<FilterCellConfigPath>/wangyou/mastercom/config</FilterCellConfigPath>
	<ResidentTime>1200</ResidentTime>
	<ResidentDis>3000</ResidentDis>
	<BuildIdCellConfigPath></BuildIdCellConfigPath>
	<LteCellHomeDistance>5000</LteCellHomeDistance>
	
	<httpPath>/sichuan/ods/nl/17102/${yyyyMMdd}/</httpPath>
	<mwPath>/sichuan/ods/wy/VOLTE/SIP/${yyyyMMdd}/</mwPath>
	<svPath>/sichuan/ods/wy/VOLTE/SV/${yyyyMMdd}/</svPath>
	<rxPath>/sichuan/ods/wy/VOLTE/GX_RX/${yyyyMMdd}/</rxPath>
	<mdtHofRlfRcefPath>/sichuan/ods/mdt/${yyyyMMdd}/</mdtHofRlfRcefPath>
	<!-- 是否输出源数据，加上定位信息  是填写yes,默认为false -->
	<outXdrData>no</outXdrData>
	<!-- 是否输出 event统计  是填写yes,默认为false -->
	<toEventData>yes</toEventData>
	
	<outSuccessData>yes</outSuccessData>
	<outFailData>yes</outFailData>
	<successProdoceStatusValue>0</successProdoceStatusValue>
	<!-- 事件统计一次跑几个小时 -->
	<TimeScheduler>6</TimeScheduler>
	
	<MergeUserResidentDayNum>-30</MergeUserResidentDayNum>
	<!-- 楼宇楼层室分用户数配置 -->
	<BuildIndoorCellUserConfigPath>/wangyou/mastercom/mroxdrmerge/resident_user/tb_cfg_building_indoor_cell</BuildIndoorCellUserConfigPath>
	<!-- 家宽配置 -->
	<HomeBroadbandConfigPath>/wangyou/mastercom/config/homebroadband.txt</HomeBroadbandConfigPath>

	<!-- 高铁区间小区配置 -->
	<hsrSectionCellPath>/wangyou/mastercom/config/hsr/tb_hsr_cfg_section_cell.txt</hsrSectionCellPath>
	<!-- 高铁站小区配置 -->
	<hsrStationCellPath>/wangyou/mastercom/config/hsr/tb_hsr_cfg_station_cell.txt</hsrStationCellPath>
	<!-- 高铁站配置 -->
	<hsrStationPath>/wangyou/mastercom/config/hsr/tb_hsr_cfg_station.txt</hsrStationPath>
	<!-- 高铁区间配置 -->
	<hsrSectionPath>/wangyou/mastercom/config/hsr/tb_hsr_cfg_section.txt</hsrSectionPath>
	<!-- 高铁路段配置 -->
	<hsrSegmentPath>/wangyou/mastercom/config/hsr/tb_hsr_cfg_segment.txt</hsrSegmentPath>
	<!-- 高铁RRU配置 -->
	<hsrSectionRruPath>/wangyou/mastercom/config/hsr/tb_hsr_cfg_rru.txt</hsrSectionRruPath>
